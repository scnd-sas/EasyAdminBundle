        "symfony/console": "^5.4|^6.4|^7.3",
        "symfony/css-selector": "^5.4|^6.4|^7.3",
        "symfony/dom-crawler": "^5.4|^6.4|^7.3",
        "symfony/phpunit-bridge": "^6.4|^7.3",
        "symfony/var-dumper": "^5.4|^6.4|^7.3",
        "symfony/yaml": "^5.4|^6.4|^7.3"
